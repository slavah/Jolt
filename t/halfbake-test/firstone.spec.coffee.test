isNodeJS = Boolean process?.pid

if isNodeJS
  path = require 'path'
  Jolt = require path.normalize process.cwd() + '/../index'
  _ = require 'underscore'

describe 'Jolt', ->
  it 'should have some properties', ->
    _keys = _.keys Jolt
    expect(_keys).toEqual [ 'Reactor', 'EventEmitter', '_', 'globalize' ]
